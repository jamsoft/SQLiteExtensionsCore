<?xml version="1.0"?>
<package >
  <metadata>
    <id>SQLiteNetExtensions-Core</id>
    <version>2.0.0-alpha2</version>
    <title>SQLite.Net Extensions-Core</title>
    <authors>JamSoft</authors>
    <owners>JamSoft</owners>
    <licenseUrl>https://bitbucket.org/twincoders/sqlite-net-extensions</licenseUrl>
    <projectUrl>https://bitbucket.org/twincoders/sqlite-net-extensions</projectUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>SQLite-Net Extensions Core is a very simple ORM that provides cascade operations, one-to-one, one-to-many, many-to-one, many-to-many, inverse and text-blobbed relationships on top of the sqlite-net library.</description>
    <releaseNotes>Changed underlying library from SQLite.Net PCL for better support on different platforms to Frank Kruegers sqlite-net-pcl</releaseNotes>
    <copyright>Copyright 2017</copyright>
    <tags>sqlite sqlite-net netstandard net extensions orm cascade recursive</tags>
    <dependencies>
      <dependency id="Newtonsoft.Json" version="9.0.1" />
      <dependency id="sqlite-net-standard" version="1.5.1" />
    </dependencies>
  </metadata>

  <files>
    <file src="SQLiteNetExtensions/bin/Release/netstandard1.5/SQLiteNetExtensions.dll" target="lib/netstandard1.5" />
    <file src="SQLiteNetExtensions/bin/Release/netstandard1.5/SQLiteNetExtensions.pdb" target="lib/netstandard1.5" />
	<file src="SQLiteNetExtensionsAsync/bin/Release/netstandard1.5/SQLiteNetExtensionsAsync.dll" target="lib/netstandard1.5" />
    <file src="SQLiteNetExtensionsAsync/bin/Release/netstandard1.5/SQLiteNetExtensionsAsync.pdb" target="lib/netstandard1.5" />
    <file src="SQLiteNetExtensions/**/*.cs" target="src" />
	<file src="SQLiteNetExtensionsAsync/**/*.cs" target="src" />
  </files>
</package>
